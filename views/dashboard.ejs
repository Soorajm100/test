
<%- include('include/_newheader') %>

<head>
    <title>Node Authentication</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <style>
     
    </style>
</head>
<body>
<div class="container">

    <div class="page-header text-center">
        <img src="<%=pic%>" width="100" height="100" alt="">
        <h1><span class="fa fa-anchor"></span> Profile Page</h1>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
    </div>

    <div class="row">

        <!-- GOOGLE INFORMATION -->
        <center>
       
            <div class="well">
                <h3 class="text-danger"><span class="fa fa-google-plus"></span> Google</h3>

                    <p>
                        <strong>name</strong>: <%= name %><br>
                        <strong>email</strong>: <%= email %><br>
                        <div>                        
                            <% num = 0 %> 
                        <% for(var i = users.length-1; i > -1; i--) { %>
                            <% num=num + 1 %> 
                           
                           
                                <% if (name==users[i].name) { %> 
                                <strong><%= users[i].gender %></strong><br>
                                <strong><%= users[i].age %></strong><br>
                                <strong><%= users[i].ip %></strong><br>
                                <strong><%= users[i].occupation %></strong><br>
                                <strong><%= users[i].phone %></strong><br>
                                <strong><%= users[i].status %></strong><br>
                                <strong><%= users[i].latitude %></strong><br>
                                <strong><%= users[i].longitude %></strong><br>
                                <strong><%= users[i].date %></strong><br>
                                <strong><%= users[i].time %></strong><br>
                                
                                <% t=1 %>
                                <% break %>
                               <% }else{ %>
                               <% t=0%>
                                
                                <% } %>

                               
                                <% } %>
                                <% if(t==0) {%>

                                    <h1>User not found</h1>
                                    <%} %>


                        </div>
                    </p>

            </div>
     
    </center>

    </div>

</div>
</body>





<%- include('include/_footer') %>